{
  "name": "lifetracker-pro",
  "version": "1.0.0",
  "private": true,
  "description": "LifeTracker Pro - A comprehensive health and wellness tracking application",
  "scripts": {
    "start:emulators": "concurrently -n \"STORAGE,COSMOS\" -c \"blue,yellow\" \"azurite --silent\" \"start \\\"\\\" \\\"C:\\Program Files\\Azure Cosmos DB Emulator\\CosmosDB.Emulator.exe\\\"\"",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "start:dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"green,cyan\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:all": "concurrently -n \"EMULATORS,SERVICES\" -c \"magenta,white\" \"npm run start:emulators\" \"npm run start:dev\"",
    "test:e2e": "start-server-and-test start:all http://localhost:3000 \"cd frontend && npx cypress run\"",
    "stop:all": "npx kill-port 3000 3001 10000 8081",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test": "npm run test:backend && npm run test:frontend",
    "lint:fix": "npm run lint:frontend -- --fix && npm run lint:backend -- --fix",
    "format": "cd frontend && npm run format && cd ../backend && npm run format",
    "cy:open": "cd frontend && npm run cy:open",
    "cy:run": "cd frontend && npm run cy:run",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start": "npm run start:all",
    "dev:mock": "cd backend && npm run dev:mock",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "update-types": "node scripts/update-types.js",
    "verify": "npm run lint && npm run test",
    "ci": "npm run verify && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/perfectlifetracker.git"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "kill-port": "^2.0.1",
    "start-server-and-test": "^2.0.11"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
