{
  "name": "lifetracker-pro-backend",
  "version": "0.1.0",
  "description": "Express TypeScript backend for LifeTracker Pro",
  "main": "dist/index.js",
  "scripts": {
    "predev": "npx kill-port 3001",
    "start": "ts-node src/index.ts",
    "start:test": "NODE_ENV=test ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:mock": "cross-env MOCK_DATA=true nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns=integration",
    "integration": "jest tests/integration",
    "coverage": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":80,\"branches\":80,\"functions\":80,\"lines\":80}}'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.{ts,json}'",
    "kill-services": "npx kill-port 3000 3001 4000 8081 10000 10001 10002",
    "emulators": "azurite --silent",
    "setup-env": "node setup-env.js",
    "postinstall": "npm run setup-env",
    "update-openai-key": "node update-openai-key.js"
  },
  "dependencies": {
    "@azure/ai-text-analytics": "^5.1.0",
    "@azure/cosmos": "^3.17.3",
    "@azure/search-documents": "^11.3.3",
    "@azure/storage-blob": "^12.14.0",
    "@types/axios": "^0.9.36",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.12",
    "applicationinsights": "^3.7.0",
    "axios": "^1.9.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "redis": "^4.6.5",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.19.87",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^10.1.2",
    "jest": "^29.5.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.10",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
