{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "2.2.0",
    "@azure/msal-browser": "4.13.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "7.1.1",
    "@mui/material": "7.1.1",
    "@react-navigation/bottom-tabs": "7.3.14",
    "@react-navigation/native": "7.1.10",
    "@react-navigation/stack": "7.3.3",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "date-fns": "3.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.79.1",
    "react-router-dom": "^7.5.0",
    "web-vitals": "^2.1.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "predev": "node ./scripts/create-env.js",
    "dev": "vite",
    "start": "vite",
    "start:clean": "powershell -ExecutionPolicy Bypass -File ./start-clean.ps1",
    "kill": "powershell -ExecutionPolicy Bypass -File ./kill-react.ps1",
    "build": "vite build",
    "build:mobile": "tsc --project tsconfig.mobile.json && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:performance": "jest --testPathPattern=__tests__/performance",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:performance": "cypress run --spec 'cypress/e2e/performance/**'",
    "test:lighthouse": "node scripts/lighthouse-test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:performance:all": "npm run test:performance && npm run test:e2e:performance && npm run test:lighthouse",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,scss}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,scss}'",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:size": "npm run build && npx bundlesize",
    "optimize:images": "npx imagemin src/assets/**/*.{jpg,png,gif} --out-dir=dist/assets",
    "deadcode": "npx ts-prune",
    "performance": "npm run build && npm run analyze && npm run deadcode",
    "clean:deps": "npx depcheck",
    "bundle:report": "npm run build -- --mode=analyze",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:preview": "npm run build && vite preview --host"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^27.5.2",
    "@types/node": "20.17.58",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "4.5.1",
    "chrome-launcher": "^1.1.0",
    "cypress": "^13.6.2",
    "depcheck": "1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "fs-extra": "11.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^24.0.0",
    "lighthouse": "12.6.1",
    "prettier": "^3.5.3",
    "puppeteer": "24.10.0",
    "rollup-plugin-visualizer": "6.0.1",
    "ts-jest": "29.3.4",
    "ts-prune": "0.10.3",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-bundle-analyzer": "0.22.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
