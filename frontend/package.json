{
  "name": "lifetracker-pro-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "React TypeScript frontend for LifeTracker Pro",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "prestart": "npx kill-port 3000",
    "start": "webpack serve --mode development",
    "predev": "npx kill-port 3000",
    "dev": "react-scripts start",
    "clean": "rimraf dist",
    "build": "webpack --mode production",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "prepare": "cd .. && husky install frontend/.husky",
    "lint-staged": "lint-staged",
    "kill-services": "npx kill-port 3000 3001 8081 10000 10001 10002"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.test.{ts,tsx}": [
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^3.0.0",
    "@azure/msal-react": "^2.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.1",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@mui/system": "^5.14.3",
    "@mui/utils": "^5.14.3",
    "@mui/x-date-pickers": "^5.0.20",
    "@mui/x-date-pickers-pro": "^8.2.0",
    "@tanstack/react-query": "^5.74.4",
    "axios": "^1.4.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.0.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "easymde": "^2.20.0",
    "jwt-decode": "^3.1.2",
    "marked": "^15.0.11",
    "primereact": "^9.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.10.1",
    "react-markdown": "^10.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.2",
    "react-simplemde-editor": "^5.2.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "styled-components": "^5.3.11",
    "typescript": "^5.1.6",
    "yup": "^1.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-datepicker": "^4.19.6",
    "@types/react-dom": "^18.0.11",
    "@types/react-toastify": "^4.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "ansi-html-community": "^0.0.8",
    "common-tags": "^1.8.2",
    "css-loader": "^6.11.0",
    "cypress": "^14.3.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-entities": "^2.6.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^2.8.7",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^2.0.11",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
